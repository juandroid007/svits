{
  "name": "svits-ts",
  "version": "0.0.0",
  "scripts": {
    "validate": "svelte-check && tsc --noEmit",
    "dev": "run-p dev:routify dev:svite",
    "dev:routify": "routify run",
    "dev:svite": "svite -ts",
    "build": "run-s build:routify build:svite",
    "build:routify": "routify run -b",
    "build:svite": "svite build -ts"
  },
  "dependencies": {
    "@roxi/routify": "^2.0.2"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "mdsvex": "^0.8.8",
    "npm-run-all": "^4.1.5",
    "svelte-check": "^1.0.53",
    "svelte-preprocess": "4.3.2",
    "svelte": "3.29.0",
    "svelte-hmr": "0.11.1",
    "svite": "^0.7.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}