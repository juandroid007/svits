{
  "name": "svits",
  "homepage": "https://svits.vercel.app",
  "author": {
    "name": "Juan Villacorta",
    "email": "juanvillacortac@gmail.com",
    "url": "https://github.com/juandroid007"
  },
  "version": "0.0.0",
  "scripts": {
    "gulp:clean": "gulp clean",
    "gulp:dev": "gulp",
    "gulp:prod": "gulp prod",
    "validate": "svelte-check && tsc --noEmit",
    "dev": "run-s gulp:dev dev:app",
    "dev:app": "run-p dev:routify dev:svite",
    "dev:routify": "routify run",
    "dev:svite": "svite -ts",
    "build": "run-s build:routify build:svite gulp:prod",
    "build:routify": "routify run -b",
    "build:svite": "NODE_ENV=production svite build -ts",
    "start": "sirv dist --host 0.0.0.0 --port 3000",
    "prod": "run-s build start"
  },
  "dependencies": {
    "svelte": "3.29.0",
    "svelte-class-transition": "^1.0.4",
    "svelte-hmr": "0.11.1",
    "@roxi/routify": "^2.0.2"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@tailwindcss/ui": "^0.6.2",
    "autoprefixer": "^9.8.6",
    "del": "^6.0.0",
    "favicons": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-webp": "^4.0.1",
    "sirv-cli": "^1.0.6",
    "sitemap": "^6.3.2",
    "mdsvex": "^0.8.8",
    "npm-run-all": "^4.1.5",
    "svelte-check": "^1.0.53",
    "svelte-preprocess": "4.3.2",
    "svite": "^0.7.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "tailwindcss": "^1.8.13",
    "vercel": "^20.1.2"
  }
}
